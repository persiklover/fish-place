{
	"compilerOptions": {
		"baseUrl": ".",
		"target": "ESNext",
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ESNext"
		],
		"paths": {
			"@/*": [
				"src/*"
			]
		},
		"noEmit": true,
		"allowJs": true,
		"esModuleInterop": false,
		"allowSyntheticDefaultImports": true,
		"module": "CommonJS",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"jsx": "react-jsx",
	}
}
